{
  "name": "tp_ets2_plugin",
  "version": "2.5.0",
  "description": "ETS2 Plugin for Touch Portal",
  "main": "src/bin/plugin.js",
  "scripts": {
    "test": "electron . --debug",
    "testNoServer": "electron . --debug --noServer",
    "test_exe": "npx electron-packager --overwrite --ignore='src/data' --ignore='src/bin/interface' --ignore='src/bin/logs' --ignore='src/bin/server' --ignore='src/bin/tmp' --ignore='src/bin/config' --ignore='src/bin/images' --ignore='src/bin/logs' --ignore='TODO.LIST' --ignore='SECURITY.md' --ignore='README.md' --ignore='LICENSE' --ignore='.gitignore' --ignore='_config.yml' --ignore='src/build' --ignore='Installers' --ignore='.vscode' --ignore='.github' . ETS2_Dashboard && node ./src/build/build.js --test",
    "release": "npx electron-packager --overwrite --ignore='src/data' --ignore='src/bin/interface' --ignore='src/bin/logs' --ignore='src/bin/server' --ignore='src/bin/tmp' --ignore='src/bin/config' --ignore='src/bin/images' --ignore='src/bin/logs' --ignore='TODO.LIST' --ignore='SECURITY.md' --ignore='README.md' --ignore='LICENSE' --ignore='.gitignore' --ignore='_config.yml' --ignore='src/build' --ignore='Installers' --ignore='.vscode' --ignore='.github' . ETS2_Dashboard && node ./src/build/build.js --release"
  },
  "bin": {
    "main": "src/bin/plugin.js"
  },
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "assets": [
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-128-black/open-sans-128-black.fnt",
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-128-black/open-sans-128-black.png"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NyboTV/TP_ETS2_Plugin.git"
  },
  "keywords": [
    "plugin",
    "tp",
    "touch",
    "portal",
    "ets2",
    "euro",
    "truck",
    "sim"
  ],
  "author": "Nybo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NyboTV/TP_ETS2_Plugin/issues"
  },
  "homepage": "https://github.com/NyboTV/TP_ETS2_Plugin#readme",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^0.27.2",
    "current-currency": "^1.6.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.2",
    "express-handlebars": "^6.0.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "get-folder-size": "^2.0.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jimp": "^0.16.1",
    "pidusage": "^3.0.0",
    "prompt": "^1.3.0",
    "replace-json-property": "^1.8.0",
    "request": "^2.88.2",
    "self-reload-json": "^0.4.0",
    "touchportal-api": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^18.2.4",
    "electron-packager": "^15.5.1"
  }
}
