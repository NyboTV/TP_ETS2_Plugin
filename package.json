{
  "name": "tp_ets2_plugin",
  "version": "1.0.0",
  "description": "ETS2 Plugin for TP",
  "main": "index.js",
  "scripts": {
    "test_index": "node ./scripts/test.js && node ./src/index.js --debug",
    "test_main": "node ./scripts/test.js && node ./src/ets2_plugin.js --debug",
    "release": "pkg . --target=node16-win-x64 --output=ets2_plugin.exe && pkg ./src/index.js --target=node16-win-x64 && node ./scripts/build.js --release"
  },
  "bin": {
    "main": "./src/ets2_plugin.js"
  },
  "pkg": {   
    "targets": [ "node16-win-x64" ], 
    "assets": [
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-128-black/open-sans-128-black.fnt",
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-128-black/open-sans-128-black.png"    
     ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NyboTV/TP_ETS2_Plugin.git"
  },
  "keywords": [
    "tp",
    "ets",
    "plugin",
    "ets2"
  ],
  "author": "Nybo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NyboTV/TP_ETS2_Plugin/issues"
  },
  "homepage": "https://github.com/NyboTV/TP_ETS2_Plugin#readme",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "basic-ftp": "^4.6.6",
    "download-github-release": "^0.3.2",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.3.3",
    "public-ip": "^4.0.4",
    "replace-json-property": "^1.8.0",
    "request": "^2.88.2",
    "self-reload-json": "^0.4.0",
    "touchportal-api": "^2.4.0-beta.5"
  }
}
