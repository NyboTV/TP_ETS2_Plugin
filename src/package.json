{
  "name": "ets2_plugin",
  "version": "1.0.0",
  "description": "TP ETS2 Plugin",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "pkg": "pkg --targets node14-win-x64 .",
    "build": "pkg --targets node14-win-x64 . && move ets2_plugin.exe ../ETS2_Dashboard/             && pkg --targets node14-win-x64 updater.js && move updater.exe ../ETS2_Dashboard/ && cd ../Releases/Plugin/ && del ETS2_Dashboard.tpp && 7z a -tzip ETS2_Dashboard.tpp ../../ETS2_Dashboard"
  },
  "bin": {
    "main": "index.js"
  },
  "keywords": [
    "TouchPortal",
    "TouchPortalPlugin"
  ],
  "author": "NyboTV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NyboTV/TP_ETS2_Plugin/issues"
  },
  "auto-updater": {
    "repo": "/NyboTV/TP_ETS2_Plugin",
    "branch": "master"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "auto-git-update": "^2.0.1",
    "download-github-release": "^0.3.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^8.1.0",
    "html2json": "^1.0.2",
    "https": "^1.0.0",
    "jimp": "^0.16.1",
    "node-html-parser": "^3.3.5",
    "octokit": "^1.0.5",
    "request": "^2.88.2",
    "touchportal-api": "^2.0.1"
  }
}
